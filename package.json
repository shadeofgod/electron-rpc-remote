{
  "name": "electron-rpc-remote",
  "version": "0.1.0",
  "description": "RPC-styled API for electron ipc.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/shadeofgod/electron-rpc-remote#readne",
  "repository": "github:shadeofgod/electron-rpc-remote",
  "bugs": "https://github.com/shadeofgod/electron-rpc-remote/issues",
  "keywords": [
    "electron",
    "ipc",
    "rpc",
    "ipcMain",
    "ipcRenderer"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "electron ./example/main.js",
    "build": "rollup -c",
    "fmt": "prettier --write \"lib/**/*\""
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/node": "^13.9.0",
    "electron": "9.1.0",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.3",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "electron": ">1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
